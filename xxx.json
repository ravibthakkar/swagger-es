        "\/_cluster\/health": {
            "get": {
                "summary": "cluster health for all indices",
                "tags": [
                    "cluster"
                ],
                "description": "http:\/\/www.elasticsearch.org\/guide\/en\/elasticsearch\/reference\/1.4\/cluster-health.html",
                "operationId": "getClusterHealth",
                "produces": [
                    "application\/json"
                ],
                "parameters": [
                    {
                        "in": "query",
                        "name": "level",
                        "description": "Can be one of cluster, indices or shards. Controls the details level of the health information returned. Defaults to cluster.",
                        "required": false,
                        "type": "string",
                        "enum": [
                            "cluster",
                            "indicies",
                            "shards"
                        ]
                    },
                    {
                        "in": "query",
                        "name": "wait_for_status",
                        "description": "One of green, yellow or red. Will wait (until the timeout provided) until the status of the cluster changes to the one provided or better, i.e. green > yellow > red. By default, will not wait for any status.",
                        "required": false,
                        "type": "string",
                        "enum": [
                            "green",
                            "yellow",
                            "red"
                        ]
                    },
                    {
                        "in": "query",
                        "name": "wait_for_relocating_shards",
                        "description": "A number controlling to how many relocating shards to wait for. Usually will be 0 to indicate to wait till all relocations have happened. Defaults to not wait.",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "wait_for_nodes",
                        "description": "The request waits until the specified number N of nodes is available. It also accepts >=N, <=N, >N and <N. Alternatively, it is possible to use ge(N), le(N), gt(N) and lt(N) notation",
                        "required": false,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ok"
                    }
                }
            }
        },
